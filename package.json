{
  "name": "GOZ-Web-Server",
  "version": "0.1.0",
  "description": "Goz web server",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run assets",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "pm2-debug": "npm run build && npm run pm2-serve-debug && npm run watch-debug",
    "pm2-serve-debug": "pm2 start ./pm2.config.js",
    "serve-debug": "nodemon --inspect dist/index.js --watch ",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
